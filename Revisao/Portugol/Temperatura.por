programa {
  funcao inicio() { //Inicio
    real tC , tF  // Variaveis
    escreva ("Escreva o valor de Temperatura Celcius \n") //Entrada de dados
    leia (tC) //Saida de dados
    tF = (tC*1.8)+32 //Processo
    escreva ("O valor da temperatura em Farenheit e: \n" +tF) // Saida de dados
  } //Fim
}
